// Function to toggle between text containers
function toggleTextContainers() {
    var textContainer = document.getElementById("textContainer");
    var newTextContainer = document.getElementById("newTextContainer");

    // Check the current display state and toggle it
    if (textContainer.style.display === "none") {
        textContainer.style.display = "block";
        newTextContainer.style.display = "none";
    } else {
        textContainer.style.display = "none";
        newTextContainer.style.display = "block";
    }
}

// Event listener for the toggle button
document.getElementById('toggleTextButton').addEventListener('click', toggleTextContainers);



